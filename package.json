{
	"name": "floof",
  "displayName": "floof",
  "publisher": "runarsf",
	"description": "Display random floof in VSCode.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.insertLink",
				"title": "Floof: Insert random link at the beginning of file"
      },
      {
        "command": "extension.renderWebview",
        "title": "Floof: Open random floof image"
      }
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runarsf/vsfloof.git"
  },
  "author": "runarsf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/runarsf/vsfloof/issues"
  },
  "homepage": "https://floof.runarsf.dev/"
}
